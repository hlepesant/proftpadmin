{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ftpgroup</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ftpgroup.id }}</td>
            </tr>
            <tr>
                <th>Groupname</th>
                <td>{{ ftpgroup.groupname }}</td>
            </tr>
            <tr>
                <th>Gid</th>
                <td>{{ ftpgroup.gid }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('ftpgroup_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('ftpgroup_edit', { 'id': ftpgroup.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
