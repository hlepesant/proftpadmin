{% extends 'base.html.twig' %}

{% block title %}FtpHistory{% endblock %}

{% block body %}
    <h1>FtpHistory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ftp_history.id }}</td>
            </tr>
            <tr>
                <th>Client_ip</th>
                <td>{{ ftp_history.clientIp }}</td>
            </tr>
            <tr>
                <th>Server_ip</th>
                <td>{{ ftp_history.serverIp }}</td>
            </tr>
            <tr>
                <th>Command</th>
                <td>{{ ftp_history.command }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ ftp_history.file }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ ftp_history.status }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ ftp_history.date ? ftp_history.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ftp_history_index') }}">back to list</a>

    <a href="{{ path('ftp_history_edit', {'id': ftp_history.id}) }}">edit</a>

    {{ include('ftp_history/_delete_form.html.twig') }}
{% endblock %}